
CREATE TABLE E_1
(
	s CHAR(18) NOT NULL 
);

CREATE TABLE E_2
(
	s CHAR(18) NOT NULL ,
	colx CHAR(18) NULL 
);

ALTER TABLE E_1
	ADD CONSTRAINT XPKE_1 PRIMARY KEY (s);

ALTER TABLE E_2
	ADD CONSTRAINT XPKE_2 PRIMARY KEY (s);

ALTER TABLE E_2
	ADD CONSTRAINT R_1 FOREIGN KEY (s) REFERENCES E_1 (s);
